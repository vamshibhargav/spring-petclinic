pipeline {
  agent any
  tools { 
        maven 'Maven'
  }
  stages {
    stage('Clone repository') {
      steps {
        checkout scm
      }
    }
    stage('Build') {
      steps {
         sh 'mvn clean package -DskipTests'
      } 
    }
    stage('createAnInstance')
    {
      steps {
          sh 'this is tmp step'
      }
    }
  }
}
